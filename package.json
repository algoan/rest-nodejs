{
  "name": "@algoan/rest",
  "version": "2.6.0",
  "description": "Algoan APIs SDK for NodeJS",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean-all": "rimraf dist/ node_modules/ coverage/ docs/",
    "build": "tsc -p .",
    "deploy:doc": "gh-pages --dist docs/ --dotfiles --message 'Update documentation [skip ci]'",
    "docs": "typedoc --out docs src/Algoan.ts --excludePrivate --excludeNotExported",
    "commit": "git-cz",
    "lint": "eslint ./src/**/*.ts --fix",
    "pretest": "npm run lint && npm run build",
    "prettier": "prettier --write '{src,test}/**/*.ts'",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:cov": "npm test -- --coverage"
  },
  "files": [
    "dist/src"
  ],
  "keywords": [
    "node",
    "typescript",
    "rest-api"
  ],
  "author": "ccoeurderoy",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ],
    "{src,test}/**/*.ts": [
      "prettier --list-different \"{src,test}/**/*.ts\""
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "cache": false,
    "extension": [
      ".ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@algoan/eslint-config": "^1.1.3",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.20",
    "axios": "^0.27.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "delay": "^5.0.0",
    "eslint": "^7.21.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.1.2",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nock": "^13.2.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.20.28",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "winston": "^3.3.3"
  },
  "peerDependencies": {
    "axios": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
